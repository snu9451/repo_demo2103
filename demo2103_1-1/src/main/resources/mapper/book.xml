<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo">
	<select id="currentTime" parameterType="java.lang.String" resultType="java.lang.String">
		SELECT TO_CHAR(sysdate, 'YYYY-MM-DD HH:MI:SS') imsi FROM dual<!-- 세미콜론 붙이지 않습니다 --><!-- date는 예약어이므로 사용NO -->
	</select>
</mapper>